{
    "name": "daviidxo/advent-of-code",
    "type": "project",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "bin-dir": "bin",
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "require": {
        "php": "^7.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Daviidxo\\AdventOfCode\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Daviidxo\\AdventOfCode\\Tests\\": "tests/src/"
        }
    }
}
